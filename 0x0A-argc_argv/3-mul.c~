#include "main.h"

/**
 * main - print the result of the multiplication, followed by a new line
 * @argc: argument count
 * @argv: argument vector / strings
 * Return: always 0
 **/

int main(int argc, char **argv)
{
	int a, times;

	if (argc < 2)
	{
		printf("Error\n");
		return (1);
	}

	else
	{
		for (a = 0; a < argc; a++)
			times = atoi(argv[1] * argv[2]);
		printf("%d\n", times);
		return (0);
	}
}
